{
  "$schema": "../../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Add my library to the project.",
      "factory": "./ng-add/index#ngAdd"
    },
    "my-service": {
      "description": "Generate a service in the project.",
      "factory": "./my-service/index#myService",
      "schema": "./my-service/schema.json"
    },
    "docs-page": {
      "description": "Generate a doc page in the project.",
      "factory": "./docs-page/index#docsPage",
      "schema": "./docs-page/schema.json"
    }
  }
}